<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="148.0" prefWidth="336.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectoud1.controller.MainController">
    <children>
        <Label text="What do you want to search?" textAlignment="CENTER">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
        <HBox alignment="CENTER" prefHeight="65.0" prefWidth="337.0">
            <children>
                <Button fx:id="btnCharactersScreen" mnemonicParsing="false" onAction="#sPeople" text="Characters">
                    <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="btnLocationScreen" mnemonicParsing="false" onAction="#sLocation" text="Locations">
                    <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>
