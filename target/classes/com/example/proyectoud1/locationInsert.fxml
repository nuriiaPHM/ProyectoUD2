         </items>
      </ComboBox>
      <ComboBox fx:id="cboxTerrain" layoutX="192.0" layoutY="181.0" prefWidth="150.0">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="City" />
               <String fx:value="Forest" />
               <String fx:value="Hill" />
               <String fx:value="Marsh" />
               <String fx:value="Mountain" />
               <String fx:value="Ocean" />
               <String fx:value="Plain" />
               <String fx:value="River" />
               <String fx:value="TODO" />
            </FXCollections>
         </items>
      </ComboBox>
      <TextField fx:id="txtWater" layoutX="192.0" layoutY="225.0" />
      <Label fx:id="lblMessage" layoutX="370.0" layoutY="200.0" />
      <Button fx:id="btnGoBack" layoutX="442.0" layoutY="250.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
   </children>
</AnchorPane>
