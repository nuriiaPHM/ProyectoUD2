<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="298.0" prefWidth="534.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectoud1.controller.LocationInsertController">
   <children>
      <Button fx:id="btnInsert" layoutX="365.0" layoutY="250.0" mnemonicParsing="false" onAction="#insert" text="INSERT" />
      <Label layoutX="104.0" layoutY="96.0" text="Name:" />
      <Label layoutX="104.0" layoutY="139.0" text="Climate:" />
      <Label layoutX="104.0" layoutY="185.0" text="Terrain:" />
      <Label layoutX="104.0" layoutY="228.0" text="Water Surface:" />
      <Label layoutX="170.0" layoutY="32.0" text="Insert your location">
