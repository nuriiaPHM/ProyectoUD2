            <Font name="DejaVu Serif Condensed Bold" size="20.0" />
         </font></Label>
      <TextField fx:id="txtName" layoutX="192.0" layoutY="92.0" />
      <ComboBox fx:id="cboxClimate" layoutX="192.0" layoutY="137.0" prefWidth="150.0">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Continental" />
               <String fx:value="Dry" />
               <String fx:value="Mild" />
               <String fx:value="Tropical" />
               <String fx:value="Wet" />
